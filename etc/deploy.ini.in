[filter-app:main]
use = egg:fanstatic#fanstatic
recompute_hashes = false
versioning = true
bottom = true
minified = true
next = inventory 

[filter:locale]
use = egg:cromlech.i18n#locale

[app:inventory]
use = egg:inventory#app
name = inventory
zcml_file = ${zcml:output}

[server:main]
use = egg:Paste#http
host = 0.0.0.0
port = 8080

